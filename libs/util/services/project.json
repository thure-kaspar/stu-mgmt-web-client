				"jestConfig": "libs/util/services/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": [
					"libs/util/services/src/**/*.ts",
					"libs/util/services/src/**/*.html"
				]
			}
		}
	},
	"tags": []
}
